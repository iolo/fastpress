layout: default
---
<h1><%= page.title %></h1>
<% if (page.date) { %>
<div>date: <%= page.date %></div>
<% } %>
<% if (page.author) { %>
<div>author: <%= page.author %></div>
<% } %>
<% if (page.tag) { %>
<div>tag: <% for (const tag of page.tag) { %><%= tag %><% } %></div>
<% } %>
<div><%- page.content %></div>
